  },
  "homepage": "https://github.com/SamHH/bukubrow",
  "dependencies": {
    "classnames": "^2.2.6",
    "purify-ts": "^0.12.2",
    "ramda": "^0.26.1",
    "react": "^16.8.0-alpha.1",
    "react-dom": "^16.8.0-alpha.1",
    "react-highlight-words": "^0.16.0",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "remote-redux-devtools": "^0.5.16",
    "reselect": "^4.0.0",
    "sanitize.css": "^8.0.0",
