{
  "name": "bukubrow",
  "version": "2.5.0",
  "description": "WebExtension for Buku",
  "main": "webextension/backend.js",
  "scripts": {
    "dev": "rm -rf ./dist/ && webpack --watch",
    "build": "rm -rf ./dist/ && NODE_ENV=production webpack",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SamHH/bukubrow.git"
