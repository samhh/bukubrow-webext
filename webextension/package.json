    "webextension-polyfill-ts": "^0.8.9"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.7",
    "@types/jest": "^23.3.12",
    "@types/ramda": "^0.25.48",
    "@types/react": "^16.7.20",
